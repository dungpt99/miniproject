{
    "extends": "prettier",
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "env": {
        "es6": true
    },
    "rules": {
        "prettier/prettier": [
            "warn",
            {
                "singleQuote": true,
                "semi": false,
                "printWidth": 120
            }
        ]
    }
}
